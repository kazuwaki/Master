<h1>Admin::Types#index</h1>
<%= form_with model:[:admin, @type] do |f| %>
  <div class="form">
    <%= f.label "タイプ種類を作成" %><br>
    <%= f.text_field :name %>
  </div>
  <div class="form">
    <%= f.submit "登録" %>
  </div>
<% end %>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @types.each do |type| %>
      <tr>
        <td><%= type.id %></td>
        <td><%= type.name %></td>
        <td><%= link_to "編集", edit_admin_type_path(type) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
