<div class="container">
  <div class="row mt-5">
    <div class="col-6 offset-3">
      <%= render "layouts/errors", obj: @alcohol %>
      <%= form_with model: [:admin, @alcohol] do |f| %>
        <div class="form">
          <%= f.label "アルコール濃度",class: "mr-4" %>
          <%= f.text_field :name,class: "mr-4" %><%= f.submit "更新内容を保存", class: "btn btn-outline-success" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
