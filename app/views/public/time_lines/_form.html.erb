<%= form_with model: time_line, local: false do |f| %>
  <div class="field">
    <%= f.label "タイトル" %>
    <%= f.text_field :title, class: "text_area" %>
  </div>
  <div class="field">
    <%= f.label "内容" %>
    <%= f.text_field :body, class: "text_area" %>
  </div>
  <div class="field">
    <%= f.submit "投稿" %>
  </div>
<% end %>