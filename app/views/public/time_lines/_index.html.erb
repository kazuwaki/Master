<% time_lines.each do |time_line| %>
  <div class="col-md-10 col-lg-10 mx-auto text-center">
    <div class="timeline-customer d-flex flex-row card p-3">
      <div class="customer-info">
        <p><%= image_tag time_line.customer.get_profile_image(100,100), class: "customers-show-images" %></p>
        <p class="post-name text-center"><%= time_line.customer.name %></p>
      </div>
      <div class="timeline">
        <p class="text-left"><%= time_line.title %><%= link_to "詳細", time_line_path(time_line), class: "btn btn-outline-secondary ml-3" %></p>
        <p class="text-left"><%= time_line.body %></p>
        <p class="text-left"><%= time_line.created_at.strftime('%Y/%m/%d %H:%M:%S') %></p>
      </div>
    </div>
  </div>
<% end %>